<div class="jumbo">
  <div class="right tomster"></div>
  <h2>Mira los post</h2>
  {{#link-to 'index' class="button"}}
    Index
  {{/link-to}}
  {{#each model as |post|}}
  <article class="listing">
    <span></span>
    <h3>{{post.id}} - {{post.title}}</h3>
    <div class="detail owner">
      {{post.body}}
    </div>
    {{#link-to 'detail' post.id class="btn btn-success"}}
      Editar
    {{/link-to}}
    
  </article>
{{/each}}
</div>